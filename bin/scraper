#!/usr/bin/env node

// Scraping backend
const Window = require('./classes/Window.js');

let window = new Window();

(async () => {
    await window.init();
    try {
        setInterval(()=> {
            window.fetchData();
        }, 5000);
    } catch (e) {
        console.log("Connection broken");
    }
    //window.close();
})();